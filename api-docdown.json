{
  "docRootDir": ".",
  "docApiDir": "docs/apis",
  "docMarkdownDir": "docs/generated",

  "generateApi": false,

  "operation": "generate",
  "showInheritedMembers": false,
  "newlineKind": "crlf",

  "markdownOptions": {
