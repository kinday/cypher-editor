    "hideEmptyTableColumns": true,
    "showBreadcrumb": true
  },

  "includePackageNames": [],
  "excludePackageNames": ["cypher-antlr4*", "cypher-extract-statements", "demo-*", "*-base", "*codemirror5*"]
}
