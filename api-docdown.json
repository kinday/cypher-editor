{
  "docRootDir": ".",
  "docApiDir": "docs/apis",
  "docMarkdownDir": "docs/generated",

  "generateApi": false,

  "operation": "generate",
  "showInheritedMembers": false,
  "newlineKind": "crlf",

  "markdownOptions": {
    "fileLevel": "package",
    "indexFilename": "index",
    "indexTitle": "API Reference",
    "indexBreadcrumbTitle": "Home",
    "showPropertyDefaults": true,
    "hideEmptyTableColumns": true,
    "showBreadcrumb": true
  },

  "includePackageNames": [],
  "excludePackageNames": [
    "antlr4*",
    "extract-statements*",
    "demo-*",
    "svelte-ts"
  ]
}
