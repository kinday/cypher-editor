{
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/plugin-transform-react-display-name": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@playwright/test": "^1.25.2",
    "cross-env": "^7.0.3",
    "lerna": "^5.5.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "rimraf packages/*/node_modules rimraf node_modules",
    "lerna": "lerna",
    "e2e:ci": "yarn e2e:prepare && yarn serve:bg && yarn e2e",
    "e2e:prepare": "lerna run build --stream && lerna run deploy --stream",
    "e2e": "lerna run e2e --stream",
    "serve:bg": "lerna run serve --stream & sleep 5",
    "serve": "lerna run serve --stream",
    "kill-serve": "npx -y kill-port 5173",
    "build-clean": "lerna run build-clean",
    "prepublishOnly": "lerna run build",
    "start": "lerna run start --scope demo --stream",
    "deploy": "lerna run build && lerna run deploy --scope demo --stream",
    "deploy-clean": "lerna run deploy-clean",
    "prettier": "prettier --write ."
  },
  "name": "cypher-editor"
}
