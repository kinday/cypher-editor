{
  "name": "demo-base",
  "description": "demo base files for cypher editor",
  "keywords": [],
  "version": "1.0.0",
  "author": "Neo Technology Inc.",
  "license": "GPL-3.0",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/neo4j-contrib/cypher-editor.git"
  },
  "bugs": {
    "url": "https://github.com/neo4j-contrib/cypher-editor/issues"
  },
  "scripts": {
    "build-lib": "babel --config-file ../../babel.config.js --extensions \".js\" --out-dir lib ./src",
    "build-es": "cross-env ES=true babel --config-file ../../babel.config.js --extensions \".js\" --out-dir es ./src",
    "build": "npm run build-lib && npm run build-es",
    "build-clean": "rimraf lib && rimraf es",
    "generate": "java -jar antlr-4.10.1-complete.jar -Dlanguage=JavaScript Cypher.g4 -o src"
  },
  "files": [
    "es/",
    "lib/",
    "README.md"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=14"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-transform-regenerator": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9"
  }
}
