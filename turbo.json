{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "start": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["lib/**", "es/**"]
    },
    "deploy": {
      "dependsOn": ["build"],
      "outputs": ["dist/**"]
    },
    "serve": {
      "dependsOn": ["deploy"]
    },
    "e2e": {
      "dependsOn": ["serve"]
    },
    "unserve": {
      "cache": false
    },
    "clean:deploy": {
      "cache": false
    },
    "clean:build": {
      "cache": false
    }
  }
}
